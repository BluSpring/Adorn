{
  "multipart": [
    {
      "when": { "facing": "north" },
      "apply": { "model": "adorn:block/fridge" }
    },
    {
      "when": { "facing": "east" },
      "apply": { "model": "adorn:block/fridge", "y": 90 }
    },
    {
      "when": { "facing": "south" },
      "apply": { "model": "adorn:block/fridge", "y": 180 }
    },
    {
      "when": { "facing": "west" },
      "apply": { "model": "adorn:block/fridge", "y": 270 }
    }
  ]
}
